{{#md-card
  onsubmit=attrs.onsubmit
  title='Register with Inventory IO'
  buttonText='Register'}}
  <div class="row">
    {{input-field
      classNames='col s12'
      type='email'
      label='Email'}}
  </div>
  <div class="row">
    {{input-field
      classNames='col s12'
      type='password'
      label='Password'}}
  </div>
  <div class="row">
    {{input-field
      classNames='col s12'
      type='password'
      label='Confirm Password'}}
  </div>
{{/md-card}}
